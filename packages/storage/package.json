{
    "name": "@funkshan/storage",
    "version": "1.0.0",
    "description": "Storage service for Hetzner S3-compatible object storage",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
